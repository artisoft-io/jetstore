# ///////////////////////////////////////////////////////////////////////////////////////
# Unit Test JetStore server process
# ---------------------------------------------------------------------------------------

# =======================================================================================
# Data Model
# ---------------------------------------------------------------------------------------
class jets:Entity {
  $base_classes = [owl:Thing],
  $data_properties = [
    jets:key as text,
    rdf:type as array of text
  ],
  $as_table = false
};

class hc:TestClaim {
  # This is an example of a domain class
  $base_classes = [jets:Entity],
  $data_properties = [
    hc:claim_number as text,
    hc:member_number as text,
    hc:code as text,
    hc:is_code as bool
  ],
  $as_table = true
};

# =======================================================================================
# Lookup Tables
# ---------------------------------------------------------------------------------------
# csv file have these columns: "CODE,DESCRIPTION"
lookup_table lk:CodeDescription {
  $csv_file = "code_lookup_test3.csv",    # csv file location
  $key = ["CODE"], 
  $columns = [
    "DESCRIPTION" as text
  ]
};

# =======================================================================================
# Metadata Triples
# ---------------------------------------------------------------------------------------
resource KEY001 = "KEY001";
resource RRConfig = "RRConfig";
resource do_codesInContractExclusion = "do_codesInContractExclusion";
triple(KEY001, rdf:type, RRConfig);
triple(KEY001, do_codesInContractExclusion, true);

