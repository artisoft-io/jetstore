# Unit Test JetStore server process
# ---------------------------------------------------------------------------------------
import "support_test2.jr"

[Rule1]:
  (?claim rdf:type hc:ZipClaim).
  [?claim exist_not hc:zip]
  ->
  (?claim hc:is_override true)
;

[Rule3]:
  (?claim rdf:type hc:ZipClaim).
  not(?claim hc:is_override true)
  ->
  (?claim hc:is_override false)
;
