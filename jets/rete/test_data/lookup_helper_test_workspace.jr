# Defining Lookup Tables
# ---------------------------------------------------------------------------------------
lookup_table acme:ProcedureLookup {
  $csv_file = "/work/buckets/mylookup.csv",    # csv file location
  $key = ["EVENT_DURATION"],
  $columns = [
    "EVENT_DURATION" as int,
    "PROC_RID" as long,
    "FROM_DATE" as date,
    "EXCL" as bool
  ]
};

