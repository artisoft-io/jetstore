class hc:Claim {
  $base_classes = [owl:Thing],
  $data_properties = [
    claim_number        as long,
    date_of_service     as date,
    point_in_time       as datetime,
    primary_diagnosis   as text,
    secondary_diagnosis as array of text,
    count               as int,
    size                as uint,
    amount              as double,
    timestamp           as ulong
  ],
  $as_table = false
};
